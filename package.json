{
  "name": "vite-plugin-execute",
  "version": "0.0.2",
  "description": "Execute your script after build",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./typings/index.d.ts",
  "exports": {
    ".": {
      "types": "./typings/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "type": "module",
  "files": [
    "dist",
    "typings"
  ],
  "scripts": {
    "dev": "vite build --mode development",
    "build": "vite build",
    "dts": "tsc --emitDeclarationOnly --declaration --declarationDir ./typings"
  },
  "author": "Jove Jonovski <jovejonovski@gmail.com> (https://github.com/datyin)",
  "license": "MIT",
  "devDependencies": {
    "@datyin/eslint-config-ts": "^0.0.7",
    "@types/node": "^18.11.9",
    "eslint": "^8.27.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.2"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "spawn",
    "run",
    "execute",
    "node",
    "electron"
  ]
}
